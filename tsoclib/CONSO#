/* REXX */
/*FTPSUB PROC FOR EXEC PREVIOUS CODE ON MULTIPLE SYSTEMS*/
ARG SYSTEMS. TEMPDS
"CONSOLE ACTIVATE NAME(A09)"
ADDRESS CONSOLE
"CART SYSCMD01"
DO I = 1 TO SYSTEMS.0
  SAY 'TRANSFERRING 'TEMPDS' TO 'SYSTEMS.I
  COMMAND="S FTPSUB,SYS="SYSTEMS.I",DS='"TEMPDS"'"
  INTERPRET '"'COMMAND'"'
  CR = GETMSG('SYSRESP.','SOL','SYSCMD01',,5)
END
ADDRESS TSO
"CONSOLE DEACTIVATE"
