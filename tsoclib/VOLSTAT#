/* REXX - SDSF/REXX CONSOLE COMMAND */
ARG  COMMAND
if ST='QSC'!ST='QNEW'!ST='QSCNEW'!ST='QUINEW'!ST='Q' then
ST = 'Q,NEW'
ELSE if ST /= 'ENABLED' & ST /= 'E' then
do
SAY 'WRONG VOLST'
EXIT 4
end
COMMAND = 'RO *ALL,v sms,vol('VOL'),'ST
IF ISFCALLS('ON') <> 0 THEN EXIT 99
ADDRESS SDSF "ISFEXEC '/"COMMAND"' (WAIT"
DO I=1 TO  ISFULOG.0
SAY  ISFULOG.I
END
CALL ISFCALLS 'OFF'
