/* REXX */
/* ROUTINE, WHICH CKECKS THAT DATASET AVIABLE */
CALL ISPPRXVP 'I'
IF ISFCALLS('ON') <> 0 THEN EXIT 99
STAT = 'NOT FOUND'
IF SYSDSN("'"TEMPDS"'")='OK' THEN
  ADDRESS TSO "DELETE '"TEMPDS"'"
IF SYSDSN("'"TEMPDS"'")<>'OK' THEN DO
  IF SYSDSN("'"DSKLST"'")='OK' THEN DO
    IF SYSDSN("'"SYSDS"'")='OK' THEN DO
      STAT='OK'
    END
  END
END
CALL ISFCALLS 'OFF'
CALL ISPPRXVP 'T'
EXIT 0
